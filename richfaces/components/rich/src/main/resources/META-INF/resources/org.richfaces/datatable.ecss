/*should be generated in M1*/
.rf-stt-exp {
    background-image: url("#{resource['org.richfaces:up_icon.gif']}");
}

.rf-stt-col {
    background-image: url("#{resource['org.richfaces:down_icon.gif']}");
}

.rf-dt-c-srt {
    cursor: pointer;
    color:'#{a4jSkin.generalLinkColor}';
}

.rf-dt-c-srt:hover {
    color:'#{a4jSkin.hoverLinkColor}';
}

.rf-dt-srt-btn {
    height: 1em;
    width: 25px;
    display: inline-block;
    vertical-align: middle;
}

.rf-dt-srt-uns {
    background: "url(#{resource['org.richfaces:sort_uns.gif']}) no-repeat center";
}

.rf-dt-srt-asc {
    background: "url(#{resource['org.richfaces:sort_asc.gif']}) no-repeat center";
}

.rf-dt-srt-des {
    background: "url(#{resource['org.richfaces:sort_des.gif']}) no-repeat center";
}

